<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kosta.myproject.model.mapper.DirectorMapper">
<insert id="register" parameterType="org.kosta.myproject.model.vo.DirectorVO">
insert into director values(director_seq.nextval,#{directorName},#{intro})
</insert>
<select id="findDirectorById" resultType="org.kosta.myproject.model.vo.DirectorVO"
 parameterType="long">
select director_id,director_name,intro from director where director_id=#{value}
</select>
</mapper>